T.c --> O(n)
S.c --> O(n)
class Solution {
    int result;
    int count = 0;
    public int kthSmallest(TreeNode root, int k) {
        dfs(root,k);
        return result;
    }
    private void dfs(TreeNode root,int k){
        if(root == null){
            return;
        }
        dfs(root.left,k);
        count++;
        if(k == count){
            result = root.val;
        }
        dfs(root.right,k);
    }
}